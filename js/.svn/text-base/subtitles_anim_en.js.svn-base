$(function () { 
  var $p = Popcorn("#video_controller").subtitle({"id":"subtitle1319736624695","start":0.801,"end":3.854,"text":"The National Film Board of Canada Presents","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624702","start":4.843,"end":6.792,"text":"The concrete residential highrise","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624708","start":6.792,"end":9.792,"text":"is the most commonly built form of the last century.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624714","start":13.793,"end":15.791,"text":"Now decades old,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624720","start":15.791,"end":19.791,"text":"these highrises are aging and falling into disrepair.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624726","start":19.871,"end":21.88,"text":"They've been labeled as failed social experiments","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624732","start":21.88,"end":23.881,"text":"from the modernist era.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624739","start":25.828,"end":26.823,"text":"So some","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624746","start":26.823,"end":27.798,"text":"are being torn down.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624751","start":28.872,"end":31.791,"text":"A few are renovated.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624757","start":31.874,"end":34.799,"text":"Most are simply left to decay.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624763","start":36.845,"end":39.853,"text":"GREATER TORONTO, 2000 RESIDENTIAL HIGHRISES","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624769","start":40.855,"end":43.793,"text":"In this Toronto suburban highrise,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624774","start":43.793,"end":45.82,"text":"we've brought together highrise residents","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624781","start":45.82,"end":48.794,"text":"with architects and animators","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624787","start":48.794,"end":50.797,"text":"to re-imagine these vertical homes","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624793","start":50.797,"end":51.87,"text":"and the spaces around them.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624800","start":51.87,"end":58.796,"text":"ONE MILLIONth TOWER","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624806","start":58.796,"end":59.795,"text":"If you drive by,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624811","start":59.795,"end":63.794,"text":"you may just see a highrise building that looks ugly,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624817","start":63.794,"end":65.85,"text":"but for us, this is our home.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624823","start":68.878,"end":70.865,"text":"Beside the building I live in,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624829","start":70.865,"end":74.849,"text":"there is a tennis court that is very dangerous.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624834","start":80.795,"end":82.834,"text":"I think this space has huge potential.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624840","start":84.792,"end":87.79,"text":"A nice, shallow, wide staircase.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624845","start":87.796,"end":89.792,"text":"So many people pass through that area,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624851","start":89.792,"end":91.792,"text":"and if we could almost change the circulation","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624857","start":91.792,"end":93.791,"text":"so that people go down through that space","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624862","start":93.791,"end":94.792,"text":"and then up and out,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624869","start":95.794,"end":97.791,"text":"it would always feel kind of active.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624874","start":97.823,"end":98.864,"text":"So you'd always have people going to school,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624880","start":98.864,"end":100.85,"text":"people going to the mall.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624886","start":101.819,"end":103.794,"text":"If you can present a vision for a neighbourhood","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624891","start":103.794,"end":104.793,"text":"that works for children,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624897","start":104.793,"end":105.795,"text":"it's probably going to work for everybody else.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624903","start":105.795,"end":107.79,"text":"Everything else, kind of follows.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624909","start":123.791,"end":124.792,"text":"Behind our building,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624915","start":124.792,"end":125.796,"text":"we have this beautiful ravine,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624920","start":125.796,"end":127.799,"text":"which has bushes and the Humber River.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624926","start":127.799,"end":132.79,"text":"It's just incredible space there which is sitting there,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624932","start":132.79,"end":135.793,"text":"it's just nothing, it just feels blank.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624938","start":140.79,"end":143.791,"text":"I enjoy gardening and when I see this empty space,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624944","start":143.791,"end":146.794,"text":"I'm sad to see that we could do something","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624949","start":146.794,"end":147.797,"text":"to make it better.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624956","start":151.797,"end":156.795,"text":"With flowers, pavement, a lot of gardens,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624961","start":156.795,"end":157.797,"text":"to make it more lively.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624968","start":157.797,"end":160.791,"text":"And flowers to make it beautiful,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624973","start":160.791,"end":162.795,"text":"because this is an amazing space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624979","start":163.796,"end":165.795,"text":"Just like the garden,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624985","start":165.795,"end":167.793,"text":"the building could be more colourful.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624990","start":167.799,"end":169.79,"text":"And green!","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736624996","start":169.794,"end":171.799,"text":"Yeah, it would be amazing if the buildings","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625002","start":171.799,"end":174.795,"text":"are more green, they are more eco-friendly.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625008","start":179.795,"end":181.792,"text":"The building is so \"bleh\"!","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625013","start":181.792,"end":182.797,"text":"You know, it's like when you come here,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625019","start":182.797,"end":183.798,"text":"you don't feel happy.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625025","start":183.798,"end":185.79,"text":"At least with colour,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625031","start":185.79,"end":187.79,"text":"you'll feel more lively coming here.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625036","start":187.79,"end":188.79,"text":"You know and it will say","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625042","start":188.79,"end":190.795,"text":"\"Yeah, come in!  Welcome!\"","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625049","start":192.793,"end":194.793,"text":"We have a lot of new immigrants.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625055","start":194.793,"end":195.795,"text":"They come and go.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625060","start":195.795,"end":197.794,"text":"But if we had a garden, maybe people","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625066","start":197.794,"end":200.793,"text":"would want to put roots down here.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625072","start":208.889,"end":212.791,"text":"The party room has been used for a church,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625077","start":212.791,"end":214.793,"text":"as well as a beauty salon.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625083","start":219.872,"end":221.849,"text":"We have all these things happening","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625089","start":221.849,"end":223.872,"text":"in such a small little space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625095","start":223.872,"end":226.882,"text":"And now, it's being used for a dance program","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625101","start":226.882,"end":229.795,"text":"that I basically facilitate.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625107","start":229.795,"end":231.79,"text":"It's called \"Youth Dance For Change\".","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625112","start":231.79,"end":235.83,"text":"And we practice hip-hop, r & b, soca and reggae.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625118","start":237.792,"end":238.792,"text":"Because the parking lot","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625124","start":238.792,"end":240.793,"text":"is in between the two buildings,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625129","start":240.793,"end":243.794,"text":"it's pretty hard for people to connect,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625136","start":243.794,"end":246.79,"text":"to even, you know, build fellowship.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625141","start":247.794,"end":249.791,"text":"It could be used for many things.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625147","start":249.791,"end":251.797,"text":"Like, cause it's such a common spot,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625153","start":251.797,"end":253.79,"text":"the plan was to actually","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625159","start":253.79,"end":254.798,"text":"get a farmers market there.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625164","start":257.844,"end":260.79,"text":"Like, you could easily build an addition,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625170","start":260.79,"end":262.793,"text":"like a two-storey addition that would come out here","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625176","start":262.793,"end":263.792,"text":"and fill out this space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625182","start":263.792,"end":266.799,"text":"It could totally be dedicated to community programming.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625188","start":270.792,"end":272.791,"text":"Our neighbourhood is dense.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625193","start":272.791,"end":275.791,"text":"Denser than most of the sections of downtown.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625199","start":276.798,"end":280.791,"text":"We really need some place to sell stuff,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625205","start":280.791,"end":281.798,"text":"to get together,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625211","start":281.798,"end":284.797,"text":"to really really build a community.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625216","start":298.794,"end":301.79,"text":"When I think about the whole neighbourhood,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625223","start":304.79,"end":305.796,"text":"we have nineteen towers,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625229","start":305.796,"end":308.791,"text":"that's about nineteen or twenty thousand people.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625234","start":308.791,"end":311.799,"text":"And there's no connection.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625240","start":316.79,"end":320.79,"text":"It just feels like we have very limited connectivity","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625246","start":320.79,"end":321.798,"text":"to all different places.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625252","start":321.798,"end":324.798,"text":"Like, from the buildings to the ravine,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625258","start":324.798,"end":327.793,"text":"or from one building to one other building.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625263","start":330.79,"end":331.799,"text":"It just feels like each tower","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625269","start":331.799,"end":334.796,"text":"is in a different continent itself.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625275","start":336.792,"end":338.798,"text":"So, it's like we're trying to make bridges","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625281","start":338.798,"end":340.798,"text":"between different towers.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625287","start":343.792,"end":346.795,"text":"But if we have accessible pedestrian-ways","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625292","start":346.795,"end":347.796,"text":"or bicycle lanes,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625298","start":347.796,"end":350.792,"text":"so that our kids and the elder people","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625304","start":350.792,"end":352.793,"text":"can go around and stroll around,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625310","start":352.793,"end":354.79,"text":"then maybe, we can connect.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625316","start":355.795,"end":358.796,"text":"As we say, that the whole world is connected now,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625322","start":358.796,"end":361.79,"text":"our world around in the neighbourhood","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1319736625328","start":361.79,"end":362.795,"text":"will be connected.","target":"subtitle-container","container":{}});
global_unitrans = $p.id
});
