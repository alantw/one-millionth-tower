var unitrans = Popcorn("#video_controller").subtitle({"id":"subtitle1311028715852","start":6.11453846153846,"end":9.64669230769231,"text":"The National Film Board of Canada Presents","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715860","start":10.539,"end":12.025,"text":"The concrete residential highrise","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715868","start":12.025,"end":15.022,"text":"is the most commonly built form of the last century.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715875","start":19.031,"end":21.019,"text":"Now decades old,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715882","start":21.019,"end":25.015,"text":"these highrises are aging and falling into disrepair.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715890","start":31.018,"end":32.034,"text":"So some","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715897","start":32.034,"end":33.089,"text":"are being torn down.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715904","start":34.098,"end":37.014,"text":"A few are renovated.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715911","start":37.094,"end":40.098,"text":"Most are simply left to decay.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715918","start":42.5513076923077,"end":45.6398076923077,"text":"GREATER TORONTO, 2000 RESIDENTIAL HIGHRISES","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715925","start":47.5475,"end":49.036,"text":"In this Toronto suburban highrise,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715932","start":49.036,"end":51.078,"text":"we've brought together highrise residents","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715938","start":51.078,"end":54.045,"text":"with architects and animators","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715945","start":54.045,"end":56.07,"text":"to re-imagine these vertical homes","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715952","start":56.07,"end":57.8060769230769,"text":"and the spaces around them.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715960","start":57.8060769230769,"end":64.07,"text":"ONE MILLIONTH TOWER","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715967","start":64.07,"end":65.057,"text":"If you drive by,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715974","start":65.057,"end":69.05,"text":"you may just see a highrise building that looks ugly,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715981","start":69.05,"end":71.084,"text":"but for us, this is our home.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715988","start":72.098,"end":74.081,"text":"Besides the buildings I live in,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028715994","start":74.081,"end":78.041,"text":"there is a tennis court that is very dangerous.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716001","start":86.058,"end":88.093,"text":"I think this space has huge potential.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716007","start":90.022,"end":93.005,"text":"A nice, shallow, wide staircase.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716013","start":93.064,"end":95.021,"text":"So many people pass through that area,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716020","start":95.021,"end":97.025,"text":"and if we could almost change the circulation","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716026","start":97.025,"end":99.018,"text":"so that people go down through that space","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716033","start":99.018,"end":100.028,"text":"and then up and out,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716040","start":101.045,"end":103.015,"text":"it would always feel kind of active.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716046","start":103.029,"end":104.076,"text":"So you'd always have people going to school,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716053","start":104.076,"end":106.058,"text":"people going to the mall.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716060","start":107.041,"end":109.042,"text":"If you can present a vision for a neighbourhood","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716067","start":109.042,"end":110.039,"text":"that works for children,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716073","start":110.039,"end":111.051,"text":"it's probably going to work for everybody else.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716080","start":111.051,"end":113.007,"text":"Everything else, kind of follows.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716087","start":129.019,"end":130.027,"text":"Behind our building,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716094","start":130.027,"end":131.066,"text":"we have this beautiful ravine,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716100","start":131.066,"end":133.091,"text":"which has bushes and the Humber River.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716106","start":133.091,"end":138.004,"text":"It's just incredible space there which is sitting there,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716113","start":138.004,"end":141.038,"text":"it's just nothing, it just feels blank.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716120","start":143.099,"end":146.006,"text":"I love to garden.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716126","start":146.006,"end":149.015,"text":"I enjoy gardening and when I see this empty space,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716133","start":149.015,"end":152.043,"text":"I'm sad to see that we could do something","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716139","start":152.043,"end":153.076,"text":"to make it better.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716145","start":157.073,"end":162.057,"text":"With flowers, pavement, a lot of gardens,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716152","start":162.057,"end":163.077,"text":"to make it more lively.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716158","start":163.077,"end":166.015,"text":"And flowers to make it beautiful,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716165","start":166.015,"end":168.055,"text":"because this is an amazing space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716171","start":169.069,"end":171.053,"text":"Just like the garden,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716178","start":171.053,"end":173.038,"text":"the building could be more colourful.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716184","start":173.095,"end":175.008,"text":"And green!","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716191","start":175.047,"end":177.099,"text":"Yeah, it would be amazing if the buildings","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716197","start":177.099,"end":180.058,"text":"are more green, they are more eco-friendly.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716203","start":185.055,"end":187.027,"text":"The building is so \"bleh\"!","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716210","start":187.027,"end":188.074,"text":"You know, it's like when you come here,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716216","start":188.074,"end":189.085,"text":"you don't feel happy.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716222","start":189.085,"end":191.01,"text":"At least with colour,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716229","start":191.01,"end":193.01,"text":"you'll feel more lively coming here.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716235","start":193.01,"end":194.01,"text":"You know and it will say","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716242","start":194.01,"end":196.054,"text":"\"Yeah, come in!  Welcome!\"","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716248","start":198.032,"end":200.04,"text":"We have a lot of new immigrants.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716255","start":200.04,"end":201.051,"text":"They come and go.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716262","start":201.051,"end":203.046,"text":"But if we had a garden, maybe people","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716268","start":203.046,"end":206.031,"text":"would want to put roots down here.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716275","start":214.075,"end":218.013,"text":"The party room has been used for a church,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716282","start":218.013,"end":220.036,"text":"as well as a beauty salon.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716288","start":221.056,"end":223.037,"text":"We have all these things happening","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716295","start":223.037,"end":225.01,"text":"in such a small little space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716302","start":225.01,"end":228.047,"text":"And now, it's being used for a dance program","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716310","start":228.047,"end":230.035,"text":"that I basically facilitate.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716317","start":230.035,"end":232.021,"text":"It's called \"Youth Dance For Change\".","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716324","start":232.021,"end":236.096,"text":"And we practice hip-hop, r & b, soca and reggae.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716331","start":243.021,"end":244.02,"text":"Because the parking lot","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716338","start":244.02,"end":246.038,"text":"is in between the two buildings,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716345","start":246.038,"end":249.042,"text":"it's pretty hard for people to connect,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716352","start":249.042,"end":252.001,"text":"to even, you know, build fellowship.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716358","start":253.047,"end":255.014,"text":"It could be used for many things.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716365","start":255.014,"end":257.076,"text":"Like, cause it's such a common spot,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716372","start":257.076,"end":259.01,"text":"the plan was to actually","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716379","start":259.01,"end":260.085,"text":"get a farmers market there.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716385","start":263.098,"end":265.086,"text":"Like, you could easily build an addition,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716392","start":265.086,"end":268.031,"text":"like a two-storey addition that would come out here","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716399","start":268.031,"end":269.027,"text":"and fill out this space.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716405","start":269.027,"end":272.093,"text":"It could totally be dedicated to community programming.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716412","start":276.023,"end":278.016,"text":"Our neighbourhood is dense.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716419","start":278.016,"end":281.014,"text":"Denser than most of the sections of downtown.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716426","start":282.089,"end":286.015,"text":"We really need some place to sell stuff,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716432","start":286.015,"end":287.083,"text":"to get together,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716439","start":287.083,"end":290.071,"text":"to really really build a community.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716445","start":304.046,"end":307.002,"text":"When I think about the whole neighbourhood,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716452","start":310.006,"end":311.06,"text":"we have nineteen towers,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716459","start":311.06,"end":314.014,"text":"that's about nineteen or twenty thousand people.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716466","start":314.014,"end":317.097,"text":"And there's no connection.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716472","start":322.01,"end":326.006,"text":"It just feels like we have very limited connectivity","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716478","start":326.006,"end":327.087,"text":"to all different places.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716485","start":327.087,"end":330.081,"text":"Like, from the buildings to the ravine,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716492","start":330.081,"end":333.034,"text":"or from one building to one other building.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716498","start":336.004,"end":337.097,"text":"It just feels like each tower","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716505","start":337.097,"end":340.067,"text":"is in a different continent itself.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716512","start":342.022,"end":344.086,"text":"So, it's like we're trying to make bridges","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716518","start":344.086,"end":346.081,"text":"between different towers.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716524","start":349.021,"end":352.059,"text":"But if we have accessible pedestrian-ways","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716531","start":352.059,"end":353.07,"text":"or bicycle lanes,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716538","start":353.07,"end":356.023,"text":"so that our kids and the elder people","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716544","start":356.023,"end":358.031,"text":"can go around and stroll around,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716551","start":358.031,"end":360.001,"text":"then maybe, we can connect.","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716558","start":361.052,"end":364.07,"text":"As we say, that the whole world is connected now,","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716564","start":364.07,"end":367.006,"text":"our world around in the neighbourhood","target":"subtitle-container","container":{}}).subtitle({"id":"subtitle1311028716571","start":367.006,"end":368.059,"text":"will be connected.","target":"subtitle-container","container":{}});
